 {
  "manifest_version": 2,
  "name": "ExtrAction",
  "version": "0.7.5",
  "permissions": [
    "activeTab",
    "identity", 
    "https://us-central1-sigma-smile-251401.cloudfunctions.net/classifier2/*"
  ],
  "description": "The goal is to be able to extract tasks and summarize them.",
  "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'",
  "icons": {"16": "img/Rainbow_Logo_square.png",
            "48": "img/Rainbow_Logo_square.png",
            "128": "img/Rainbow_Logo_square.png" },
  "browser_action": {
   "default_icon": "img/Rainbow_Logo_square.png",
   "default_title": "Working model of the baseline extension",
   "default_popup": "index.html"
  },
  "key": ""
}

